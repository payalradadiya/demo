Welcome <strong><%= current_user.email %></strong>.
<div class="center-block text-center">
<h1>Photo</h1>
</div>

<div class="masonry">
      <% @photos.each do |photo| %>
      <div class="item">
      <!-- Renders image -->
      <%= link_to image_tag(photo.image.url, class: 'photoitem'), photo.image.url, target: '_blank' %>
      <div class="center-block text-center">
        <h4><%= photo.title %></h4>
      
      <%= link_to 'Remove', photo_path(photo), class: 'btn btn-danger', method: :delete, data: {confirm: 'Are you sure?'} %>
      </div>
      </div>
      <% end %>
</div>